	$(function() {
		$("#btn").bind("click", function(){
			
			var str="";
			
			$("li").each(function () {  		//test()수행   //test:호출
				str += $(this).text() + "<br/>";
				
			});
			
			//$(str).appendTo(""#box");
			$("#box").text(str);				// .innerText 같다.	
			//$("#box").html(str);              // .innerHTML 같다.
			
		});	
	});

</script>

