<title>Insert title here</title>
	<style type="text/css">
	
	.st { color:blue;}
	</style>
	
	<script type="text/javascript">
		var i=0;
		
		for (;i<5;i++){
			document.write("<p class='st'>"+(i+1)+"</p>")
		}
		document.writeln("---------------------"+i+1);
		
		for (var k=0;k<10;k++){
			document.write("<h2>"+k+"</h2>")
		}
		document.writeln("---------------------"+k+1);
	</script>
